$userPath = [System.Environment]::GetEnvironmentVariable("PATH", "User")
$systemPath = $systemPath -replace "C:\\Program Files\\Python39\\Scripts\\;", ""
$systemPath = $systemPath -replace "C:\\Program Files\\Python39\\;", ""
$userPath = $userPath -replace "C:\\Program Files\\Python39\\Scripts\\;", ""
$userPath = $userPath -replace "C:\\Program Files\\Python39\\;", ""
