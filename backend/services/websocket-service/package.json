{
  "name": "sallie-websocket-service",
  "version": "1.0.0",
  "description": "Sallie Studio WebSocket Service - Real-time communication hub",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "db:reset": "knex migrate:rollback:all && knex migrate:latest && knex seed:run"
  },
  "keywords": [
    "websocket",
    "real-time",
    "communication",
    "sallie-studio",
    "microservice"
  ],
  "author": "Sallie Studio Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "socket.io": "^4.7.4",
    "ioredis": "^5.3.2",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "prom-client": "^15.0.0",
    "opentelemetry-api": "^1.7.0",
    "opentelemetry-sdk": "^1.18.1",
    "opentelemetry-instrumentation-express": "^0.38.0",
    "opentelemetry-instrumentation-http": "^0.45.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
